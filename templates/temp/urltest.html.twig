{% extends 'base.html.twig' %}

{% block title %}Dealer's Thing - Test{% endblock %}

{% block header %}
    {% include 'header.html.twig' with user %}
{% endblock %}
{% block sidebar %}
    {% include 'sidebar.html.twig' with user %}
{% endblock %}
{% block body %}
    <style>
        .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
        .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
    </style>

    <div class="col-10 offset-1 d-flex align-items-center justify-content-center pt-5">
        <div class="card col-12" id="register-staff-screen">
            <div class="card-header">
                <h2>Images</h2>
            </div>
            <div class="card-body col-12 row">
                    {% for i in images %}
                            <div class="col-4 box-alt-color"><img class="img-fluid" src="{{ i.url }}" alt="{{ i.url }}" /></div>
                    {% endfor %}
            </div>
        </div>
    </div>
{% endblock %}
